    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "less": "^3.10.3",
    "normalize.css": "^8.0.1",
    "parcel-bundler": "^1.12.3",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10",
    "vuex": "^3.1.1"
  },
  "posthtml": {
    "recognizeSelfClosing": true

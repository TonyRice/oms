    "directory": "packages/omg"
  },
  "license": "MIT",
  "dependencies": {
    "@types/event-kit": "^2.4.0",
    "commander": "^3.0.0",
    "event-kit": "^2.5.3",
    "omg-ui": "^0.14.1",
    "omg-validate": "^0.14.1"
  },
  "devDependencies": {
    "typescript": "^3.1.6"
  }
}

{
  "name": "omg",
  "version": "0.10.2",
  "private": true,
  "devDependencies": {
    "lerna": "^3.13.4",
    "yarn": "^1.16.0"
  },
  "scripts": {
    "bootstrap": "lerna bootstrap --npm-client=yarn",
    "lint": "lerna run lint",
    "test": "lerna run test",
    "coverage": "lerna run coverage",
    "build": "lerna run build",
    "test-post-build": "lerna run test-post-build",
    "publish": "yarn bootstrap && yarn build && lerna publish --from-package",
    "version": "lerna version --no-git-tag-version --no-push"
  },
  "bin": "omg"
}
